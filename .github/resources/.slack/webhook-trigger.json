{
  "type": "webhook",
  "name": "Post the latest actions from GitHub",
  "description": "Share event information to a channel",
  "workflow": "#/workflows/event_notifications",
  "inputs": {
    "author": {
      "value": "{{data.author}}"
    },
    "channel_id": {
      "value": "{{data.channel_id}}"
    },
    "repo_name": {
      "value": "{{data.repo_name}}"
    },
    "status": {
      "value": "{{data.status}}"
    },
    "event_url": {
      "value": "{{data.event_url}}"
    }
  }
}
